{
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "require": {
    "ext-pdo": "*",
    "doctrine/dbal": "^4.0",
    "doctrine/migrations": "^3.7",
    "doctrine/orm": "^3.0",
    "php-di/php-di": "^7.0",
    "slim/psr7": "^1.6",
    "slim/twig-view": "^3.3",
    "symfony/cache": "^7.0",
    "symfony/console": "^7.0",
    "symfony/twig-bridge": "^7.0",
    "symfony/webpack-encore-bundle": "^2.1",
    "twig/intl-extra": "^3.8",
    "twig/twig": "^3.8",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  }
}
